# common functions
hsearch() { history | grep $1 | grep -v hsearch; }

function phpstrict() {
	if [ "$1" != "" ]
	then
		'php' -d error_reporting=2048 -f $1
	else
		echo "Usage: phpstrict <file>"
	fi
}

function cdl() {
	'cd' $1
	'ls' -Ahlrtp --color=always
}

function mkcd()
{
	'mkdir' $*
	'cd' ${@: -1}
}

function title()
{
    printf "\033k$1\033\\"
}
